<% provide(:title, 'Home') %>

<div class="container-fluid">
  <div class="span8">
    <% if signed_in? %>
      <section>
      <%= render 'shared/link_form' %>
      </section>
        <ol class="links">
          <%= render @link_users %>
        </ol>
        <%= will_paginate @link_users %>

    <% else %>
      <% cache "front", expires_in: 10.minutes do %>
      <h4> Front Page </h4>
      <ol class="links">
        <%= render @link_users if @link_users %>
      </ol>
    <% end %>
  <% end %>
  </div>  
</div>
