<% if @link_users.all.count>0 %>
  $('.links').append('<%= escape_javascript(render(partial: 'link_users/link_user', collection: @link_users)) %>');

  <% if !(@link_users.total_pages == @link_users.current_page) %>
    $('a.kooh').attr('href', '<%= user_path(@user,page: (params[:page].to_i || 1)+1, per_page: params[:per_page] || 15) %>').show();
  <% end %>
<% else -%>
  $('a.hook-topic').hide();
<% end -%>
