<% provide(:title, @user.name) %>
<div class="container">
  <div class="span8">
    <h4><%= @title %> 
      (<%= @users.count %>) </h4>
    <% if @users.any? %>
      <ul class="users">
        <%= render partial: 'users/user', collection: @users, cache: true %>
      </ul>
      <%= will_paginate @users %>
    <% end %>
  </div>
</div>
