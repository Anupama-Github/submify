<% provide(:title, @user.user_name) %>
<div class="row-fluid">
  <div class="span8">
    <% tab "followers" %>
    <%= render 'profile_stat_nav' %>
      <ul class="users">
        <%= render @users if @users.any? %>
      </ul>
      <ol class="show-commented"></ol>
      <ol class="links"></ol>
        <div class="load-more">
          <%= link_to 'Load More', followers_user_path(@user,page: (params[:page].to_i || 1)+1, per_page: params[:per_page] || 15), class: 'kooh', remote: true %>
        </div>
  </div>
</div>
