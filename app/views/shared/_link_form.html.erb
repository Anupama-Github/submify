
<%= form_for(@link) do |f| %>
  <div class="field">
    <%= f.text_area :url_link, placeholder: "Submit a link...", id: "complete_url"%>
    <%= text_field_tag 'topic_name',nil, class: 'link-topic-tag' %>
    <%= f.submit "Submit", class: "btn btn-small", id: "submit_button"%>
  </div>
<% end %>


