
<% if link.users.exists? current_user %>
	<%= form_for link, :html => { :method => :delete }, remote: true do |f| %>
          <%= f.hidden_field :id, value: link.id %>
		<%= f.submit "Unsubmit" %>
	<% end %>
<% else %>
	<%= form_for :link, url: submit_link_path, remote: true do |f| %>
                <%= f.hidden_field :id, value: link.id %>
		<%= f.submit "submit" %>
	<% end %>
<% end %>
